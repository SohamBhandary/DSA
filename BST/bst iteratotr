class BSTIterator {
    private Stack<TreeNode>st= new Stack<>();
    public BSTIterator(TreeNode root) {
        pushAll(root);        
    }    
    public int next() {
        TreeNode curr = st.pop();
        pushAll(curr.right);
        return curr.val;      
    }
    public boolean hasNext() {
        return !st.isEmpty();       
    }
    public void pushAll(TreeNode root){
        while(root!=null){
            st.push(root);
            root=root.left;
        }
    }
}
